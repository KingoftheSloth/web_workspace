package dao;

import org.apache.ibatis.session.SqlSession;

import dto.animal;
import myBatis.SqlSessionBean;

public class animalDao {
	public animal insert(desertionNo,kind, breed, neuterYn, processState, happenDt) {
		SqlSession mapperSession = SqlSessionBean.getSession();
		animal vo = mapperSession.insert(null);
		
		return vo;
	}
}
