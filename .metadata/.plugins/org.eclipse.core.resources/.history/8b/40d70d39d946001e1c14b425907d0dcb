    request.setCharacterEncoding("UTF-8");    
 String desertionNo =request.getParameter("desertionNo");
 List<Animal> animalList = new ArrayList<>();
Animal animal = Animal.builder()
.desertionNo(request.getParameter("derstionNo")
.kind(request.getParameter("kind"))
.breed(request.getParameter("breed"))
.neuterYn(request.getParameter("neuterYn"))
.processState(request.getParameter("processState"))
.happenDt(request.getParameter("happenDt"))
.age(request.getParameter("age"))
.filename(request.getParameter("filename"))
.weight(request.getParameter("weight"))
.build();
animalList.add(animal);
animalDao.getInstance().insert(animal);
%>
